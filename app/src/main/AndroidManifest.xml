<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.android.firebaseauth">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FirebaseAuth"
        android:usesCleartextTraffic="true"
        tools:replace="android:appComponentFactory"
        android:appComponentFactory="whateverString">

        <activity android:name=".views.ResetPwdActivity"></activity>
        <activity android:name=".views.SignInActivity" />
        <activity android:name=".views.CreateAccountActivity" />
        <activity android:name=".views.SwuInActivity"/>
        <activity android:name=".views.SwuOutActivity"/>
        <activity android:name=".views.ThemaActivity"/>
        <activity android:name=".views.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".views.swu_in.In_CafeMapFragmentActivity"/>
        <activity android:name=".views.swu_in.In_FoodMapFragmentActivity"/>
        <activity android:name=".views.swu_in.In_PhotoMapFragmentActivity"/>
        <activity android:name=".views.swu_in.In_RestMapFragmentActivity"/>
        <activity android:name=".views.swu_in.In_StudyMapFragmentActivity"/>

        <activity android:name=".views.swu_out.FoodMapFragmentActivity"/>
        <activity android:name=".views.swu_out.CafeMapFragmentActivity"/>
        <activity android:name=".views.swu_out.WalkMapFragmentActivity"/>
        <activity android:name=".views.swu_out.StudyMapFragmentActivity"/>
        <activity android:name=".views.swu_out.PlayMapFragmentActivity"/>

        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="2nv9b63rbh" />

    </application>

</manifest>